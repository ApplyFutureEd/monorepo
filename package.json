{
    "name": "students",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "next dev",
        "build": "next build && next export",
        "storybook": "start-storybook -p 6006",
        "analyse:dev": "ANALYZE_BUNDLE=true yarn start",
        "analyse:prod": "ANALYZE_BUNDLE=true next build",
        "analyse:unused": "next-unused",
        "commit": "git add . && cz",
        "lint": "eslint --fix .",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "test": "jest",
        "test:coverage": "jest && curl -s https://codecov.io/bash -t 71bd4721-25ce-45c8-b597-839e27b55808",
        "security:audit": "yarn audit",
        "packages:upgrade": "yarn upgrade-interactive --latest"
    },
    "next-unused": {
        "alias": {},
        "debug": true,
        "include": [
            "src"
        ],
        "exclude": [
            ".test.",
            "src/aws-exports.js"
        ],
        "entrypoints": [
            "src/pages"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./**/*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "jest": {
        "collectCoverage": true,
        "coveragePathIgnorePatterns": [
            ".story.tsx"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "1.2.32",
        "@fortawesome/pro-light-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "0.1.12",
        "classnames": "2.2.6",
        "next": "10.0.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-i18next": "11.7.3",
        "webfontloader": "1.6.28"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "3.0.0",
        "@next/bundle-analyzer": "10.0.1",
        "@storybook/addon-knobs": "^6.1.0-beta.6",
        "@storybook/react": "^6.1.0-beta.6",
        "@tailwindcss/ui": "0.7.2",
        "@testing-library/jest-dom": "5.11.5",
        "@testing-library/react": "11.1.1",
        "@types/classnames": "2.2.11",
        "@types/node": "14.14.7",
        "@types/react": "16.9.56",
        "@types/storybook__react": "^5.2.1",
        "@typescript-eslint/eslint-plugin": "4.7.0",
        "@typescript-eslint/parser": "4.7.0",
        "@welldone-software/why-did-you-render": "6.0.0-rc.1",
        "@zeit/next-source-maps": "0.0.4-canary.1",
        "autoprefixer": "9.8.5",
        "babel-jest": "26.6.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "7.13.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.21.5",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-plugin-simple-import-sort": "5.0.3",
        "husky": "4.3.0",
        "jest": "26.6.3",
        "lint-staged": "10.5.1",
        "next-unused": "0.0.3",
        "path": "0.12.7",
        "postcss-custom-properties": "10.0.0",
        "postcss-flexbugs-fixes": "5.0.1",
        "postcss-preset-env": "6.7.0",
        "prettier": "2.1.2",
        "prettier-plugin-tailwind": "2.1.9",
        "stylelint-config-recommended": "3.0.0",
        "tailwindcss": "1.9.6",
        "tailwindcss-filters": "3.0.0",
        "typescript": "4.0.5"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
